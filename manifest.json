{
    "manifest_version": 2, 
    "name": "Zoom Link Tracker", 
    "description": "A tracker which allows you to access all of you recoccurinng Zoom Meetings in one place.", 
    "version": "1.0.0", 
    "icons" : {}, 
    "content_scripts":[{
        "matches" : ["<all_urls>"],
        "js" : ["script.js"]
    }],
    "background":{
        "js" : ["background.js"]
    },
    "browser_action":{
        "default_icon": "unnamed.png", 
        "default_popup": "index.html"
    }, 
    "permissions" : ["activeTab", "storage"]
}